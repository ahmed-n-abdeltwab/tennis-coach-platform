{
  "name": "auth-helpers",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/auth-helpers/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "dist/libs/auth-helpers",
        "main": "libs/auth-helpers/src/index.ts",
        "tsConfig": "libs/auth-helpers/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/auth-helpers/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/auth-helpers"],
      "options": {
        "jestConfig": "libs/auth-helpers/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
