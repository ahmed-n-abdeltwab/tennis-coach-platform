{
  "extends": "../../tsconfig.base.json",
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./",
    "paths": {
      "@app/*": ["src/app/*"],
      "@common/*": ["src/common/*"],
      "@config/*": ["src/config/*"],
      "@auth/*": ["src/app/auth/*"],
      "@users/*": ["src/app/users/*"],
      "@sessions/*": ["src/app/sessions/*"],
      "@bookings/*": ["src/app/booking-types/*"],
      "@coaches/*": ["src/app/coaches/*"],
      "@calendar/*": ["src/app/calendar/*"],
      "@messages/*": ["src/app/messages/*"],
      "@payments/*": ["src/app/payments/*"],
      "@notifications/*": ["src/app/notifications/*"],
      "@discounts/*": ["src/app/discounts/*"],
      "@time-slots/*": ["src/app/time-slots/*"],
      "@health/*": ["src/app/health/*"],
      "@prisma/*": ["src/app/prisma/*"],
      "@dto/*": ["src/common/dto/*"],
      "@decorators/*": ["src/common/decorators/*"],
      "@controllers/*": ["src/common/controllers/*"],
      "@test-utils/*": ["test/utils/*"],
      "@test/*": ["test/*"],
      "/*": ["*"]
    }
  }
}
